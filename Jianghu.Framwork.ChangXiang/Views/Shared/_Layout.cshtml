<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=GB2312" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href="~/Content/images/index.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/css/common.css" rel="stylesheet" />
    <style type="text/css">
        .cont12 {
            height: 70px;
            width: 722px;
        }

        .cont11 {
            width: 698px;
            height: 216px;
            margin-left: 10px;
            font-size: 13px;
        }

        #cont13 {
            color: #FF0000;
        }

        #cont14 {
            color: #FF0000;
        }

        #cont15 {
            color: #00FF99;
        }

        #cont16 {
            color: #00FF99;
        }

        #cont17 {
            color: #FF0099;
        }

        #cont18 {
            color: #FF0099;
        }

        #cont19 {
            color: #33FFCC;
        }

        #cont20 {
            color: #33FFCC;
        }

        #cont21 {
            color: #666600;
        }

        #cont22 {
            color: #666600;
        }

        #cont23 {
            color: #009933;
        }

        #cont24 {
            color: #009933;
        }

        #cont13, #cont15, #cont17, #cont19, #cont21, #cont23 {
            font-size: 14px;
            font-weight: bold;
        }

        .STYLE1 {
            font-size: larger;
            color: #CC9999;
        }

        .STYLE2 {
            font-size: larger;
            color: #FFFF00;
            font-weight: bold;
        }

        .STYLE3 {
            font-size: larger;
            color: #66FF00;
            font-weight: bold;
        }
    </style>
</head>
<body>
    <div class="main">
        <div class="main2">
            <!--顶部-->
            <div class="top"></div>
            <div class="top2">
                <object classid="clsid:D27CDB6E-AE6D-11cf-96B8-444553540000" codebase="" width="100%" height="99%">
                    <param name="movie" value="~/Content/images/logo.swf" />
                    <param name="quality" value="high" />
                    <param name="wmode" value="transparent" />
                    <embed src="~/Content/images/logo.swf" wmode="transparent" quality="high" pluginspage="" type="application/x-shockwave-flash" width="999" height="160"></embed>
                </object>
            </div>
            <div class="top3"></div>
            @RenderBody()
        </div>
    </div>
    <script src="~/Scripts/jquery-1.8.2.min.js"></script>
    <script type="text/javascript">
        $("#verifyImg").click(function () {
            $(this).attr("src", '@Url.Action("ValidateCode","Common")?time=' + (new Date()).getTime());
        });
    </script>
    @RenderSection("scripts", required: false)
</body>
</html>
